{
    "name": "sylver35/ajaxchecks",
    "type": "phpbb-extension",
    "description": "Breizh Ajax Checks Extension for phpBB. Allows checking ajax availability of username, email address. Power of the password and respect obligations. In the registration (mode=register) page and the account modification page (reg_details).",
    "homepage": "https://breizhcode.com",
    "version": "1.7.0",
    "time": "2020-04-05",
    "keywords": ["phpbb", "extension", "password", "email", "register", "profile", "ajax", "checks", "breizh"],
    "license": "GPL-2.0-only",
    "authors": [{
            "name": "Sylver35",
            "email": "webmaster@breizhcode.com",
            "homepage": "https://breizhcode.com",
            "role": "Developer"
        }],
    "require": {
        "php": ">=5.4",
        "ext-json": "*",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.2.9"
    },
    "extra": {
        "display-name": "Breizh Ajax Checks",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.9"
        },
        "version-check": {
            "host": "versions.breizhcode.com",
            "directory": "/ajaxchecks",
            "filename": "/version_check.json"
        }
    }
}
